#define _CRT_SECURE_NO_WARNINGS 1
/*编写并测试Fibonacci()函数，该函数用循环代替递归计算斐波那契
数。*/
  
#include<stdio.h>
#include<ctype.h>
int main()
{
	int i = 0;
	int j = 0;
	int sum = 0;
	int times = 0;
	int n;
	scanf("%d", &n);
	for (i = 1, j = 1; times < n; times++)
	{
		sum = i + j;
		j = i;
		i = sum;
	}
	printf("%d", sum);
	return 0;

}

